CONTROL AUTOMATON ErrorPath1

INITIAL STATE ARG0;

STATE USEFIRST ARG0 :
    MATCH "" -> GOTO ARG1;
    TRUE -> STOP;

STATE USEFIRST ARG1 :
    MATCH "void assert(int cond)" -> GOTO ARG2_1_1;
STATE USEFIRST ARG2_0_1 :
    MATCH "void assert(int cond)" -> GOTO ARG2_1_1;
STATE USEFIRST ARG2_1_1 :
    MATCH "int __VERIFIER_nondet_int();" -> GOTO ARG2_2_1;
STATE USEFIRST ARG2_2_1 :
    MATCH "int main()" -> GOTO ARG2_3_1;
STATE USEFIRST ARG2_3_1 :
    MATCH "" -> GOTO ARG2_4_1;
STATE USEFIRST ARG2_4_1 :
    MATCH "int x = __VERIFIER_nondet_int();" -> GOTO ARG2_5_1;
STATE USEFIRST ARG2_5_1 :
    MATCH "int x = __VERIFIER_nondet_int();" -> GOTO ARG2_6_1;
STATE USEFIRST ARG2_6_1 :
    MATCH "int z = 1;" -> ASSUME {x == (0);} GOTO ARG2;
    TRUE -> STOP;

STATE USEFIRST ARG2 :
    MATCH "[!((x * x) < 0)]" -> GOTO ARG4;
    TRUE -> STOP;

STATE USEFIRST ARG4 :
    MATCH "[!(x > 0)]" -> GOTO ARG6;
    TRUE -> STOP;

STATE USEFIRST ARG6 :
    MATCH "z = 3;" -> GOTO ARG7;
    TRUE -> STOP;

STATE USEFIRST ARG7 :
    MATCH "return 0;" -> ERROR;
    TRUE -> STOP;

STATE USEFIRST ARG9 :
    TRUE -> STOP;

END AUTOMATON
