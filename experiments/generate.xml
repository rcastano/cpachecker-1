<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="cpachecker"
           cpuCores="1"
           memlimit="12000 MB">
  <option name="-heap">8000M</option>

  <tasks name="original">
    <includesfile>../my-programs/instances.set</includesfile>
  </tasks>

  <rundefinition name="generate-explicit-10800">
    <option name="-setprop">limits.time.cpu=10800s</option>
    <option name="-setprop">assumptions.automatonFile=../${sourcefile_path}/${sourcefile_name}.explicit.assumption_automaton</option>
    <option name="-config">${sourcefile_path}/../../../test-configs/generate/explicitAnalysis-generate-cmc-condition.properties</option>
    <option name="-skipRecursion"/>
  </rundefinition>

  <rundefinition name="generate-explicit-900">
    <option name="-setprop">limits.time.cpu=900s</option>
    <option name="-setprop">assumptions.automatonFile=../${sourcefile_path}/${sourcefile_name}.explicit.assumption_automaton</option>
    <option name="-config">${sourcefile_path}/../../../test-configs/generate/explicitAnalysis-generate-cmc-condition.properties</option>
    <option name="-skipRecursion"/>
  </rundefinition>

  <rundefinition name="generate-predicate-10800">
    <option name="-setprop">limits.time.cpu=10800s</option>
    <option name="-setprop">assumptions.automatonFile=../${sourcefile_path}/${sourcefile_name}.predicate.assumption_automaton</option>
    <option name="-config">${sourcefile_path}/../../../test-configs/generate/predicateAnalysis-generate-cmc-condition.properties</option>
    <option name="-skipRecursion"/>
  </rundefinition>

  <rundefinition name="generate-predicate-900">
    <option name="-setprop">limits.time.cpu=900s</option>
    <option name="-setprop">assumptions.automatonFile=../${sourcefile_path}/${sourcefile_name}.predicate.assumption_automaton</option>
    <option name="-config">${sourcefile_path}/../../../test-configs/generate/predicateAnalysis-generate-cmc-condition.properties</option>
    <option name="-skipRecursion"/>
  </rundefinition>

  <columns>
    <column title="total">time for CPAchecker</column>
    <column title="reached">reached set</column>
    <column title="memory">Heap memory usage</column>
  </columns>
</benchmark>
