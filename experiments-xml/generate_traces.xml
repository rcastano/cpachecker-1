<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="generate_random_walk_traces"
           timelimit="900s"
           hardtimelimit="1000s"
           cpuCores="1"
           memlimit="12000 MB">
  <option name="-heap">8000M</option>

  <tasks name="unfinished-instances">
	  <includesfile>../my-programs/generated_seeds.set</includesfile>
  </tasks>

  <rundefinition name="generate_traces">
  </rundefinition>

  <columns>
    <column title="total">time for CPAchecker</column>
    <column title="reached">reached set</column>
    <column title="memory">Heap memory usage</column>
  </columns>
</benchmark>
