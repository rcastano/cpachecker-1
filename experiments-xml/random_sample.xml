<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.0//EN" "http://www.sosy-lab.org/benchexec/benchmark-1.0.dtd">
<benchmark tool="coverage_from_random_sample_of_traces"
           cpuCores="1"
           memlimit="12000 MB">
  <option name="-heap">8000M</option>

  <tasks name="unfinished-explicit-900">
	  <includesfile>../my-programs/unfinished_explicit_instances.set</includesfile>
	  <option name="--assumption_automaton_suffix">.explicit.assumption_automaton</option>
  </tasks>
  <tasks name="unfinished-predicate-900">
    <includesfile>../my-programs/unfinished_predicate_instances.set</includesfile>
	  <option name="--assumption_automaton_suffix">.predicate.assumption_automaton</option>
  </tasks>

  <rundefinition name="coverage_random_sample">
  </rundefinition>

  <columns>
    <column title="total">time for CPAchecker</column>
    <column title="reached">reached set</column>
    <column title="memory">Heap memory usage</column>
    <column title="total_lines">lines to cover</column>
    <column title="covered_lines">Covered</column>
    <column title="traces_found">Traces used</column>
  </columns>
</benchmark>
